@article{galeana-sanchez_existence_2013,
	title = {On the existence and number of (k+1)-kings in k-quasi-transitive digraphs},
	volume = {313},
	issn = {0012-365X},
	url = {http://www.sciencedirect.com/science/article/pii/S0012365X13003440},
	doi = {10.1016/j.disc.2013.08.007},
	abstract = {Let D=(V(D),A(D)) be a digraph and k≥2 an integer. We say that D is k-quasi-transitive if for every directed path (v0,v1,…,vk) in D we have (v0,vk)∈A(D) or (vk,v0)∈A(D). Clearly, a 2-quasi-transitive digraph is a quasi-transitive digraph in the usual sense. Bang-Jensen and Gutin proved that a quasi-transitive digraph D has a 3-king if and only if D has a unique initial strong component, and if D has a 3-king and the unique initial strong component of D has at least three vertices, then D has at least three 3-kings. In this paper we prove the following generalization: a k-quasi-transitive digraph D has a (k+1)-king if and only if D has a unique initial strong component, and if D has a (k+1)-king, then either all the vertices of the unique initial strong components are (k+1)-kings or the number of (k+1)-kings in D is at least (k+2). Also, we obtain new results on the minimum number of 3-kings in quasi-transitive digraphs.},
	pages = {2582--2591},
	number = {22},
	journaltitle = {Discrete Mathematics},
	shortjournal = {Discrete Mathematics},
	author = {Galeana-Sánchez, Hortensia and Hernández-Cruz, César and Juárez-Camacho, Manuel Alejandro},
	urldate = {2018-02-08},
	date = {2013-11-28},
	keywords = {Digraph, -king, -quasi-transitive digraph, Quasi-transitive digraph},
	file = {Galeana-Sánchez et al_2013_On the existence and number of (k+1)-kings in k-quasi-transitive digraphs.pdf:C\:\\Users\\zcsxo\\Zotero\\storage\\YWAA6SDG\\Galeana-Sánchez et al_2013_On the existence and number of (k+1)-kings in k-quasi-transitive digraphs.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\zcsxo\\Zotero\\storage\\UCP322VJ\\Galeana-Sánchez et al. - 2013 - On the existence and number of (k+1)-kings in k-qu.html:text/html}
}

