@article{bang-jensen_vertex_1997,
	title = {Vertex heaviest paths and cycles in quasi-transitive digraphs},
	volume = {163},
	issn = {0012-365X},
	url = {http://www.sciencedirect.com/science/article/pii/0012365X9500318Q},
	doi = {10.1016/0012-365X(95)00318-Q},
	abstract = {A digraph D is called a quasi-transitive digraph ({QTD}) if for any triple x,y,z of distinct vertices of D such that (x, y) and (y, z) are arcs of D there is at least one arc from x and z or from z to x. Solving a conjecture by Bang-Jensen and Huang (1995), Gutin (1995) described polynomial algorithms for finding a Hamiltonian cycle and a Hamiltonian path (if it exists) in a {QTD}. The approach taken in that paper cannot be used to find a longest path or cycle in polynomial time. We present a principally new approach that leads to polynomial algorithms for finding vertex heaviest paths and cycles in {QTDs} with non-negative weights on the vertices. This, in particular, provides an answer to a question by N. Alon on longest paths and cycles in {QTDs}.},
	pages = {217--223},
	number = {1},
	journaltitle = {Discrete Mathematics},
	shortjournal = {Discrete Mathematics},
	author = {Bang-Jensen, Jørgen and Gutin, Gregory},
	urldate = {2018-02-12},
	date = {1997-01-15},
	file = {Bang-Jensen_Gutin_1997_Vertex heaviest paths and cycles in quasi-transitive digraphs.pdf:C\:\\Users\\zcsxo\\Zotero\\storage\\YZUAL677\\Bang-Jensen_Gutin_1997_Vertex heaviest paths and cycles in quasi-transitive digraphs.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\zcsxo\\Zotero\\storage\\TVBVAUGP\\Bang-Jensen and Gutin - 1997 - Vertex heaviest paths and cycles in quasi-transiti.html:text/html}
}

