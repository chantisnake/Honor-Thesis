@article{koh_kings_1995,
	title = {Kings in multipartite tournaments},
	volume = {147},
	issn = {0012-365X},
	url = {http://www.sciencedirect.com/science/article/pii/0012365X9400169J},
	doi = {10.1016/0012-365X(94)00169-J},
	abstract = {Let T be an n-partite tournament and let kr(T) denote the number of r-kings of T. Gutin (1986) and Petrovic and Thomassen (1991) proved independently that if T contains at most one transmitter, then k4(T) ⩾ 1, and found infinitely many bipartite tournaments T with at most one transmitter such that k3 (T) = 0. In this paper, we (i) obtain some sufficient conditions for T to have k3 (T) ⩾ 1, (ii) show that if T contains no transmitter, then k4 (T) ⩾ 4 when n = 2, and k4 (T) ⩾ 3 when n ⩾ 3, and (iii) characterize all T with no transmitter such that the equalities in (ii) hold.},
	pages = {171--183},
	number = {1},
	journaltitle = {Discrete Mathematics},
	shortjournal = {Discrete Mathematics},
	author = {Koh, K. M. and Tan, B. P.},
